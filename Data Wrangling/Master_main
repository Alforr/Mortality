# Tabla hombre/mujer ahora
{
  tabla_92_hombre <- defs %>%
    filter(Ano_fa == 1992, Sexo == 1) %>% 
    group_by(Edad, tramo_esc) %>% # homme
    summarise(Count = n(), .groups = 'drop') %>% 
    pivot_wider(names_from = tramo_esc, values_from = Count, values_fill = list(Count = 0)) 
  tabla_92_mujer <- defs %>%
    filter(Ano_fa == 1992, Sexo == 2) %>% 
    group_by(Edad, tramo_esc) %>% # mulher
    summarise(Count = n(), .groups = 'drop') %>% 
    pivot_wider(names_from = tramo_esc, values_from = Count, values_fill = list(Count = 0)) 
  #Partamos por hombre:
  {
    #rm(tabla_92_hombre)
    tabla_sub_100 <- tabla_92_hombre %>%
      filter(Edad <= 99)
    tabla_op_99 <- tabla_92_hombre %>%
      filter(Edad > 99)
    sumar_sobre_99 <- tabla_op_99 %>%
      summarise(
        Edad = 99,
        alta = sum(alta,na.rm = TRUE),
        media = sum(media, na.rm = TRUE),
        baja = sum(baja, na.rm = TRUE)
      )
    tabla_92_hombre <- tabla_sub_100 %>%
      group_by(Edad) %>%
      summarise(
        alta = sum(alta, na.rm = TRUE),
        baja = sum(baja, na.rm = TRUE),
        media = sum(media, na.rm = TRUE)
      ) %>%
      bind_rows(sumar_sobre_99) %>%
      arrange(Edad)
    tabla_92_hombre <- df_hombre_92 %>% #tengamos los valores de muerte y vida juntos
      left_join(tabla_92_hombre, by = "Edad") %>%
      replace_na(list(Cantidad = 0)) %>%
      arrange(Edad)
    tabla_92_hombre <- tabla_92_hombre %>%
      select(-year)
    tabla_92_hombre <- tabla_92_hombre %>% #death rate bajo
      mutate(mx_bajo = ifelse(esc_bajo != 0, baja / esc_bajo, NA))
    tabla_92_hombre <- tabla_92_hombre %>% #death rate medio
      mutate(mx_medio = ifelse(esc_medio != 0, media / esc_medio, NA))
    tabla_92_hombre <- tabla_92_hombre %>% #death rate alto
      mutate(mx_alto = ifelse(esc_alto != 0, alta / esc_alto, NA))
    tabla_92_hombre <- tabla_92_hombre %>% #death rate central de todos
      mutate(mx_st = ifelse(esc_bajo != 0, (baja+media+alta)/(esc_bajo+esc_medio+esc_alto),NA))
    #Borremos algunas columnas que sobran: NA, year
    tabla_92_hombre <- tabla_92_hombre %>%
      mutate(esc_totales = esc_bajo+esc_medio+esc_alto)
    tabla_92_hombre <- tabla_92_hombre %>%
      mutate(totales = alta+media+baja)
    tabla_92_hombre <- tabla_92_hombre %>%
      mutate(prop_alto = esc_alto/esc_totales)
    tabla_92_hombre <- tabla_92_hombre %>%
      mutate(prop_medio = esc_medio/esc_totales)
    tabla_92_hombre <- tabla_92_hombre %>%
      mutate(prop_bajo = esc_bajo/esc_totales)
    tabla_92_hombre <- tabla_92_hombre %>% filter(Edad >= 25 & Edad <= 99 )
    tabla_92_hombre <- tabla_92_hombre %>%
      slice(1:(n() - 1))
    }
    
  #Grafiquemos interacciones
  {
    plot_esc_alta <- ggplot(tabla_92_hombre, aes(x = Edad, y = prop_alto)) +
      geom_line(color = "blue") +
      labs(title = "Proporcion escolaridad alta por edad: 1992 (hombres)",
           x = "Age",
           y = "Proporcion esc_alta") +
      theme_minimal()
    plot_esc_alta
    plot_esc_media <- ggplot(tabla_92_hombre, aes(x = Edad, y = prop_medio)) +
      geom_line(color = "blue") +
      labs(title = "Proporcion escolaridad media por edad: 1992 (hombres)",
           x = "Age",
           y = "Proporcion esc_media") +
      theme_minimal()
    plot_esc_media
    plot_esc_baja <- ggplot(tabla_92_hombre, aes(x = Edad, y = prop_bajo)) +
      geom_line(color = "blue") +
      labs(title = "Proporcion escolaridad baja por edad: 1992 (hombres)",
           x = "Age",
           y = "Proporcion esc_baja") +
      theme_minimal()
    plot_esc_baja
  }
  # Armemos la TV: central primero
  {
    TV_92_H_t <- data.frame(matrix(nrow = nrow(tabla_92_hombre), ncol = 0))
    TV_92_H_t$Edad <- tabla_92_hombre$Edad
    TV_92_H_t$Mx <- tabla_92_hombre$mx_st
    TV_92_H_t <- TV_92_H_t %>% filter(Edad >= 25 & Edad <= 99)
    fn <- 0.5 #Distribucion normal de muertes en el año, simplifica la vida y calculos
    TV_92_H_t <- TV_92_H_t %>%
      mutate(qx = ifelse(Mx != 0, Mx / (1 + fn * Mx), 0)) # Setup probabilidad de muerte en un año
    TV_92_H_t$qx[nrow(TV_92_H_t)] <- 1
    TV_92_H_t$lx[1] <- 100000
    TV_92_H_t$dx[1] <- TV_92_H_t$lx[1] * TV_92_H_t$qx[1]
    for (i in 2:nrow(TV_92_H_t)) { #Armar las columnas de lx  y dx
      TV_92_H_t$lx[i] <- TV_92_H_t$lx[i-1] - TV_92_H_t$dx[i-1]
      TV_92_H_t$dx[i] <- TV_92_H_t$lx[i] * TV_92_H_t$qx[i]
    } 
    TV_92_H_t$Lx <- NA # Armemos Lx
    for (i in 1:(nrow(TV_92_H_t) - 1)) {
      TV_92_H_t$Lx[i] <- (TV_92_H_t$lx[i] + TV_92_H_t$lx[i + 1]) / 2
    }
    last_row <- nrow(TV_92_H_t) # Truncar los valores w-esimos segun Chiang (1972)
    TV_92_H_t$Lx[last_row] <- TV_92_H_t$lx[last_row] / TV_92_H_t$Mx[last_row]
    TV_92_H_t$Tx <- NA # Armemos Tx
    TV_92_H_t$Tx[last_row] <- TV_92_H_t$Lx[last_row] #se parte del final con el valor terminal de Lx = lx/mx
    for (i in (last_row - 1):1) { #se itera con lx + Tx+1(edad siguiente)
      TV_92_H_t$Tx[i] <- TV_92_H_t$Lx[i] + TV_92_H_t$Tx[i + 1]
    }
    TV_92_H_t$P_x1 <- NA #Armemos P(x,1)
    TV_92_H_t$P_x1[1] <- TV_92_H_t$Lx[1] / TV_92_H_t$lx[1] #Primer valor como Lx/lx
    for (i in 2:nrow(TV_92_H_t)) { # Valores consecutivos como Tx/T(x-1)
      TV_92_H_t$P_x1[i] <- TV_92_H_t$Tx[i] / TV_92_H_t$Tx[i - 1]
    }
    TV_92_H_t <- TV_92_H_t %>%
      mutate(e_0 = ifelse(Tx != 0, Tx / lx )) #se logro armar la tabla, ahora hay que transformarla en funcion.
    
  }
  # Baja
  {
    TV_92_H_b <- data.frame(matrix(nrow = nrow(tabla_92_hombre), ncol = 0))
    TV_92_H_b$Edad <- tabla_92_hombre$Edad
    TV_92_H_b$Mx <- tabla_92_hombre$mx_bajo
    TV_92_H_b <- TV_92_H_b %>% filter(Edad >= 25 & Edad <= 99)
    fn <- 0.5 #Distribucion normal de muertes en el año, simplifica la vida y calculos
    TV_92_H_b <- TV_92_H_b %>%
      mutate(qx = ifelse(Mx != 0, Mx / (1 + fn * Mx), 0)) # Setup probabilidad de muerte en un año
    TV_92_H_b$qx[nrow(TV_92_H_b)] <- 1
    TV_92_H_b$lx[1] <- 100000
    TV_92_H_b$dx[1] <- TV_92_H_b$lx[1] * TV_92_H_b$qx[1]
    for (i in 2:nrow(TV_92_H_b)) { #Armar las columnas de lx  y dx
      TV_92_H_b$lx[i] <- TV_92_H_b$lx[i-1] - TV_92_H_b$dx[i-1]
      TV_92_H_b$dx[i] <- TV_92_H_b$lx[i] * TV_92_H_b$qx[i]
    } 
    TV_92_H_b$Lx <- NA # Armemos Lx
    for (i in 1:(nrow(TV_92_H_b) - 1)) {
      TV_92_H_b$Lx[i] <- (TV_92_H_b$lx[i] + TV_92_H_b$lx[i + 1]) / 2
    }
    last_row <- nrow(TV_92_H_b) # Truncar los valores w-esimos segun Chiang (1972)
    TV_92_H_b$Lx[last_row] <- TV_92_H_b$lx[last_row] / TV_92_H_b$Mx[last_row]
    TV_92_H_b$Tx <- NA # Armemos Tx
    TV_92_H_b$Tx[last_row] <- TV_92_H_b$Lx[last_row] #se parte del final con el valor terminal de Lx = lx/mx
    for (i in (last_row - 1):1) { #se itera con lx + Tx+1(edad siguiente)
      TV_92_H_b$Tx[i] <- TV_92_H_b$Lx[i] + TV_92_H_b$Tx[i + 1]
    }
    TV_92_H_b$P_x1 <- NA #Armemos P(x,1)
    TV_92_H_b$P_x1[1] <- TV_92_H_b$Lx[1] / TV_92_H_b$lx[1] #Primer valor como Lx/lx
    for (i in 2:nrow(TV_92_H_b)) { # Valores consecutivos como Tx/T(x-1)
      TV_92_H_b$P_x1[i] <- TV_92_H_b$Tx[i] / TV_92_H_b$Tx[i - 1]
    }
    TV_92_H_b <- TV_92_H_b %>%
      mutate(e_0 = ifelse(Tx != 0, Tx / lx )) #se logro armar la tabla, ahora hay que transformarla en funcion.
  }
  # Media, por no suavizar e(0) de media > e(0) de alta
  {
    TV_92_H_m <- data.frame(matrix(nrow = nrow(tabla_92_hombre), ncol = 0))
    TV_92_H_m$Edad <- tabla_92_hombre$Edad
    TV_92_H_m$Mx <- tabla_92_hombre$mx_medio
    TV_92_H_m <- TV_92_H_m %>% filter(Edad >= 25 & Edad <= 99)
    fn <- 0.5 #Distribucion normal de muertes en el año, simplifica la vida y calculos
    TV_92_H_m <- TV_92_H_m %>%
      mutate(qx = ifelse(Mx != 0, Mx / (1 + fn * Mx), 0)) # Setup probabilidad de muerte en un año
    TV_92_H_m$qx[nrow(TV_92_H_m)] <- 1
    TV_92_H_m$lx[1] <- 100000
    TV_92_H_m$dx[1] <- TV_92_H_m$lx[1] * TV_92_H_m$qx[1]
    for (i in 2:nrow(TV_92_H_m)) { #Armar las columnas de lx  y dx
      TV_92_H_m$lx[i] <- TV_92_H_m$lx[i-1] - TV_92_H_m$dx[i-1]
      TV_92_H_m$dx[i] <- TV_92_H_m$lx[i] * TV_92_H_m$qx[i]
    } 
    TV_92_H_m$Lx <- NA # Armemos Lx
    for (i in 1:(nrow(TV_92_H_m) - 1)) {
      TV_92_H_m$Lx[i] <- (TV_92_H_m$lx[i] + TV_92_H_m$lx[i + 1]) / 2
    }
    last_row <- nrow(TV_92_H_m) # Truncar los valores w-esimos segun Chiang (1972)
    TV_92_H_m$Lx[last_row] <- TV_92_H_m$lx[last_row] / TV_92_H_m$Mx[last_row]
    TV_92_H_m$Tx <- NA # Armemos Tx
    TV_92_H_m$Tx[last_row] <- TV_92_H_m$Lx[last_row] #se parte del final con el valor terminal de Lx = lx/mx
    for (i in (last_row - 1):1) { #se itera con lx + Tx+1(edad siguiente)
      TV_92_H_m$Tx[i] <- TV_92_H_m$Lx[i] + TV_92_H_m$Tx[i + 1]
    }
    TV_92_H_m$P_x1 <- NA #Armemos P(x,1)
    TV_92_H_m$P_x1[1] <- TV_92_H_m$Lx[1] / TV_92_H_m$lx[1] #Primer valor como Lx/lx
    for (i in 2:nrow(TV_92_H_m)) { # Valores consecutivos como Tx/T(x-1)
      TV_92_H_m$P_x1[i] <- TV_92_H_m$Tx[i] / TV_92_H_m$Tx[i - 1]
    }
    TV_92_H_m <- TV_92_H_m %>%
      mutate(e_0 = ifelse(Tx != 0, Tx / lx )) #se logro armar la tabla, ahora hay que transformarla en funcion.
  }
  # Alta
  {
    TV_92_H_a <- data.frame(matrix(nrow = nrow(tabla_92_hombre), ncol = 0))
    TV_92_H_a$Edad <- tabla_92_hombre$Edad
    TV_92_H_a$Mx <- tabla_92_hombre$mx_alto
    TV_92_H_a <- TV_92_H_a %>% filter(Edad >= 25 & Edad <= 99)
