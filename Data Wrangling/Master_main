      for (i in (last_row - 1):1) { #se itera con lx + Tx+1(edad siguiente)
        TV_92_M_b$Tx[i] <- TV_92_M_b$Lx[i] + TV_92_M_b$Tx[i + 1]
      }
      TV_92_M_b$P_x1 <- NA #Armemos P(x,1)
      TV_92_M_b$P_x1[1] <- TV_92_M_b$Lx[1] / TV_92_M_b$lx[1] #Primer valor como Lx/lx
      for (i in 2:nrow(TV_92_M_b)) { # Valores consecutivos como Tx/T(x-1)
        TV_92_M_b$P_x1[i] <- TV_92_M_b$Tx[i] / TV_92_M_b$Tx[i - 1]
      }
      TV_92_M_b <- TV_92_M_b %>%
        mutate(e_0 = ifelse(Tx != 0, Tx / lx )) #se logro armar la tabla, ahora hay que transformarla en funcion.
    }
    # Media
    {
      TV_92_M_m <- data.frame(matrix(nrow = nrow(tabla_92_hombre), ncol = 0))
      TV_92_M_m$Edad <- tabla_92_hombre$Edad
      TV_92_M_m$Mx <- tabla_92_hombre$mx_medio
      TV_92_M_m <- TV_92_M_m %>% filter(Edad >= 25 & Edad <= 99)
      fn <- 0.5 #Distribucion normal de muertes en el año, simplifica la vida y calculos
      TV_92_M_m <- TV_92_M_m %>%
        mutate(qx = ifelse(Mx != 0, Mx / (1 + fn * Mx), 0)) # Setup probabilidad de muerte en un año
      TV_92_M_m$qx[nrow(TV_92_M_m)] <- 1
      TV_92_M_m$lx[1] <- 100000
      TV_92_M_m$dx[1] <- TV_92_M_m$lx[1] * TV_92_M_m$qx[1]
      for (i in 2:nrow(TV_92_M_m)) { #Armar las columnas de lx  y dx
        TV_92_M_m$lx[i] <- TV_92_M_m$lx[i-1] - TV_92_M_m$dx[i-1]
        TV_92_M_m$dx[i] <- TV_92_M_m$lx[i] * TV_92_M_m$qx[i]
      } 
      TV_92_M_m$Lx <- NA # Armemos Lx
      for (i in 1:(nrow(TV_92_M_m) - 1)) {
        TV_92_M_m$Lx[i] <- (TV_92_M_m$lx[i] + TV_92_M_m$lx[i + 1]) / 2
      }
      last_row <- nrow(TV_92_M_m) # Truncar los valores w-esimos segun Chiang (1972)
      TV_92_M_m$Lx[last_row] <- TV_92_M_m$lx[last_row] / TV_92_M_m$Mx[last_row]
      TV_92_M_m$Tx <- NA # Armemos Tx
      TV_92_M_m$Tx[last_row] <- TV_92_M_m$Lx[last_row] #se parte del final con el valor terminal de Lx = lx/mx
      for (i in (last_row - 1):1) { #se itera con lx + Tx+1(edad siguiente)
        TV_92_M_m$Tx[i] <- TV_92_M_m$Lx[i] + TV_92_M_m$Tx[i + 1]
      }
      TV_92_M_m$P_x1 <- NA #Armemos P(x,1)
      TV_92_M_m$P_x1[1] <- TV_92_M_m$Lx[1] / TV_92_M_m$lx[1] #Primer valor como Lx/lx
      for (i in 2:nrow(TV_92_M_m)) { # Valores consecutivos como Tx/T(x-1)
        TV_92_M_m$P_x1[i] <- TV_92_M_m$Tx[i] / TV_92_M_m$Tx[i - 1]
      }
      TV_92_M_m <- TV_92_M_m %>%
        mutate(e_0 = ifelse(Tx != 0, Tx / lx )) #se logro armar la tabla, ahora hay que transformarla en funcion.
    }
    # Alta
    {
      TV_92_M_a <- data.frame(matrix(nrow = nrow(tabla_92_hombre), ncol = 0))
      TV_92_M_a$Edad <- tabla_92_hombre$Edad
      TV_92_M_a$Mx <- tabla_92_hombre$mx_alto
      TV_92_M_a <- TV_92_M_a %>% filter(Edad >= 25 & Edad <= 99)
      fn <- 0.5 #Distribucion normal de muertes en el año, simplifica la vida y calculos
      TV_92_M_a <- TV_92_M_a %>%
        mutate(qx = ifelse(Mx != 0, Mx / (1 + fn * Mx), 0)) # Setup probabilidad de muerte en un año
      TV_92_M_a$qx[nrow(TV_92_M_a)] <- 1
      TV_92_M_a$lx[1] <- 100000
      TV_92_M_a$dx[1] <- TV_92_M_a$lx[1] * TV_92_M_a$qx[1]
      for (i in 2:nrow(TV_92_M_a)) { #Armar las columnas de lx  y dx
        TV_92_M_a$lx[i] <- TV_92_M_a$lx[i-1] - TV_92_M_a$dx[i-1]
        TV_92_M_a$dx[i] <- TV_92_M_a$lx[i] * TV_92_M_a$qx[i]
      } 
      TV_92_M_a$Lx <- NA # Armemos Lx
      for (i in 1:(nrow(TV_92_M_a) - 1)) {
        TV_92_M_a$Lx[i] <- (TV_92_M_a$lx[i] + TV_92_M_a$lx[i + 1]) / 2
      }
      last_row <- nrow(TV_92_M_a) # Truncar los valores w-esimos segun Chiang (1972)
      TV_92_M_a$Lx[last_row] <- TV_92_M_a$lx[last_row] / TV_92_M_a$Mx[last_row]
      TV_92_M_a$Tx <- NA # Armemos Tx
      TV_92_M_a$Tx[last_row] <- TV_92_M_a$Lx[last_row] #se parte del final con el valor terminal de Lx = lx/mx
      for (i in (last_row - 1):1) { #se itera con lx + Tx+1(edad siguiente)
        TV_92_M_a$Tx[i] <- TV_92_M_a$Lx[i] + TV_92_M_a$Tx[i + 1]
      }
      TV_92_M_a$P_x1 <- NA #Armemos P(x,1)
      TV_92_M_a$P_x1[1] <- TV_92_M_a$Lx[1] / TV_92_M_a$lx[1] #Primer valor como Lx/lx
      for (i in 2:nrow(TV_92_M_a)) { # Valores consecutivos como Tx/T(x-1)
        TV_92_M_a$P_x1[i] <- TV_92_M_a$Tx[i] / TV_92_M_a$Tx[i - 1]
      }
      TV_92_M_a <- TV_92_M_a %>%
        mutate(e_0 = ifelse(Tx != 0, Tx / lx )) #se logro armar la tabla, ahora hay que transformarla en funcion.
    } 
    }
  # Guardemos las tablas 
  setwd("C:/Users/Alfonso Orrego/Documents/GitHub/Mortality/Results")
  write.csv(TV_92_M_b, "TV_92_M_b.csv", row.names = FALSE)
  write.csv(TV_92_M_m, "TV_92_M_m.csv", row.names = FALSE)
  write.csv(TV_92_M_a, "TV_92_M_a.csv", row.names = FALSE)
  write.csv(TV_92_M_t, "TV_92_M_t.csv", row.names = FALSE)
    
    
  }
  