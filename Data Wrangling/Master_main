    setwd("C:/Users/Alfonso Orrego/Documents/GitHub/Mortality/Results")
    write.csv(TV_92_totales_media, "TV_92_totales_media.csv", row.names = FALSE)
    ggsave("lx_totales_92_media.png", plot = plot_lx, width = 8, height = 6)
    ggsave("qx_totales_92_media.png", plot = plot_qx, width = 8, height = 6)
    #ggsave("tx_totales_92_media.png", plot = tx, width = 8, height = 6)
    ggsave("e0x_totales_92_media.png", plot = plot_e_0, width = 8, height = 6)
  }
