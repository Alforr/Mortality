    for (i in 2:nrow(TV_92_H_a)) { # Valores consecutivos como Tx/T(x-1)
      TV_92_H_a$P_x1[i] <- TV_92_H_a$Tx[i] / TV_92_H_a$Tx[i - 1]
    }
    TV_92_H_a <- TV_92_H_a %>%
      mutate(e_0 = ifelse(Tx != 0, Tx / lx )) #se logro armar la tabla, ahora hay que transformarla en funcion.
  }
  # Guardemos las tablas :
  # Guardemos las tablas 
  setwd("C:/Users/Alfonso Orrego/Documents/GitHub/Mortality/Results")
  write.csv(TV_92_H_b, "TV_92_H_b.csv", row.names = FALSE)
  write.csv(TV_92_H_m, "TV_92_H_m.csv", row.names = FALSE)
  write.csv(TV_92_H_a, "TV_92_H_a.csv", row.names = FALSE)
  write.csv(TV_92_H_t, "TV_92_H_t.csv", row.names = FALSE)
  #Ahora las mujeres:
  {
    {
      #rm(tabla_92_hombre)
      tabla_sub_100 <- tabla_92_mujer %>%
        filter(Edad <= 99)
      tabla_op_99 <- tabla_92_mujer %>%
        filter(Edad > 99)
      sumar_sobre_99 <- tabla_op_99 %>%
        summarise(
          Edad = 99,
          alta = sum(alta,na.rm = TRUE),
          media = sum(media, na.rm = TRUE),
          baja = sum(baja, na.rm = TRUE)
        )
      tabla_92_mujer <- tabla_sub_100 %>%
        group_by(Edad) %>%
        summarise(
          alta = sum(alta, na.rm = TRUE),
          baja = sum(baja, na.rm = TRUE),
          media = sum(media, na.rm = TRUE)
        ) %>%
        bind_rows(sumar_sobre_99) %>%
        arrange(Edad)
      tabla_92_mujer <- df_hombre_92 %>% #tengamos los valores de muerte y vida juntos
        left_join(tabla_92_mujer, by = "Edad") %>%
        replace_na(list(Cantidad = 0)) %>%
        arrange(Edad)
      tabla_92_mujer <- tabla_92_mujer %>%
        select(-year)
      tabla_92_mujer <- tabla_92_mujer %>% #death rate bajo
        mutate(mx_bajo = ifelse(esc_bajo != 0, baja / esc_bajo, NA))
      tabla_92_mujer <- tabla_92_mujer %>% #death rate medio
        mutate(mx_medio = ifelse(esc_medio != 0, media / esc_medio, NA))
      tabla_92_mujer <- tabla_92_mujer %>% #death rate alto
        mutate(mx_alto = ifelse(esc_alto != 0, alta / esc_alto, NA))
      tabla_92_mujer <- tabla_92_mujer %>% #death rate central de todos
        mutate(mx_st = ifelse(esc_bajo != 0, (baja+media+alta)/(esc_bajo+esc_medio+esc_alto),NA))
      #Borremos algunas columnas que sobran: NA, year
      tabla_92_mujer <- tabla_92_mujer %>%
        mutate(esc_totales = esc_bajo+esc_medio+esc_alto)
      tabla_92_mujer <- tabla_92_mujer %>%
        mutate(totales = alta+media+baja)
      tabla_92_mujer <- tabla_92_mujer %>%
        mutate(prop_alto = esc_alto/esc_totales)
      tabla_92_mujer <- tabla_92_mujer %>%
        mutate(prop_medio = esc_medio/esc_totales)
      tabla_92_mujer <- tabla_92_mujer %>%
        mutate(prop_bajo = esc_bajo/esc_totales)
      tabla_92_mujer <- tabla_92_mujer %>% filter(Edad >= 25 & Edad <= 99 )
      tabla_92_mujer <- tabla_92_mujer %>%
        slice(1:(n() - 1))
    }
    # Ahora por las TV: central, y por tramos.
    # Central
    {
      TV_92_M_t <- data.frame(matrix(nrow = nrow(tabla_92_hombre), ncol = 0))
      TV_92_M_t$Edad <- tabla_92_hombre$Edad
      TV_92_M_t$Mx <- tabla_92_hombre$mx_st
      TV_92_M_t <- TV_92_M_t %>% filter(Edad >= 25 & Edad <= 99)
      fn <- 0.5 #Distribucion normal de muertes en el año, simplifica la vida y calculos
      TV_92_M_t <- TV_92_M_t %>%
        mutate(qx = ifelse(Mx != 0, Mx / (1 + fn * Mx), 0)) # Setup probabilidad de muerte en un año
      TV_92_M_t$qx[nrow(TV_92_M_t)] <- 1
      TV_92_M_t$lx[1] <- 100000
      TV_92_M_t$dx[1] <- TV_92_M_t$lx[1] * TV_92_M_t$qx[1]
      for (i in 2:nrow(TV_92_M_t)) { #Armar las columnas de lx  y dx
        TV_92_M_t$lx[i] <- TV_92_M_t$lx[i-1] - TV_92_M_t$dx[i-1]
        TV_92_M_t$dx[i] <- TV_92_M_t$lx[i] * TV_92_M_t$qx[i]
      } 
      TV_92_M_t$Lx <- NA # Armemos Lx
      for (i in 1:(nrow(TV_92_M_t) - 1)) {
        TV_92_M_t$Lx[i] <- (TV_92_M_t$lx[i] + TV_92_M_t$lx[i + 1]) / 2
      }
      last_row <- nrow(TV_92_M_t) # Truncar los valores w-esimos segun Chiang (1972)
      TV_92_M_t$Lx[last_row] <- TV_92_M_t$lx[last_row] / TV_92_M_t$Mx[last_row]
      TV_92_M_t$Tx <- NA # Armemos Tx
      TV_92_M_t$Tx[last_row] <- TV_92_M_t$Lx[last_row] #se parte del final con el valor terminal de Lx = lx/mx
      for (i in (last_row - 1):1) { #se itera con lx + Tx+1(edad siguiente)
        TV_92_M_t$Tx[i] <- TV_92_M_t$Lx[i] + TV_92_M_t$Tx[i + 1]
      }
      TV_92_M_t$P_x1 <- NA #Armemos P(x,1)
      TV_92_M_t$P_x1[1] <- TV_92_M_t$Lx[1] / TV_92_M_t$lx[1] #Primer valor como Lx/lx
      for (i in 2:nrow(TV_92_M_t)) { # Valores consecutivos como Tx/T(x-1)
        TV_92_M_t$P_x1[i] <- TV_92_M_t$Tx[i] / TV_92_M_t$Tx[i - 1]
      }
      TV_92_M_t <- TV_92_M_t %>%
        mutate(e_0 = ifelse(Tx != 0, Tx / lx )) #se logro armar la tabla, ahora hay que transformarla en funcion.
      
    }
    # Baja
    {
      TV_92_M_b <- data.frame(matrix(nrow = nrow(tabla_92_hombre), ncol = 0))
      TV_92_M_b$Edad <- tabla_92_hombre$Edad
      TV_92_M_b$Mx <- tabla_92_hombre$mx_bajo
      TV_92_M_b <- TV_92_M_b %>% filter(Edad >= 25 & Edad <= 99)
