        TV_92_M_b$dx[i] <- TV_92_M_b$lx[i] * TV_92_M_b$qx[i]
      } 
      TV_92_M_b$Lx <- NA # Armemos Lx
      for (i in 1:(nrow(TV_92_M_b) - 1)) {
        TV_92_M_b$Lx[i] <- (TV_92_M_b$lx[i] + TV_92_M_b$lx[i + 1]) / 2
      }
      last_row <- nrow(TV_92_M_b) # Truncar los valores w-esimos segun Chiang (1972)
      TV_92_M_b$Lx[last_row] <- TV_92_M_b$lx[last_row] / TV_92_M_b$Mx[last_row]
      TV_92_M_b$Tx <- NA # Armemos Tx
